{
  "name": "coordinator",
  "version": "1.0.0",
  "description": "Coordinator Microservice - Central registry and configuration hub",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "create:feature": "node scripts/create-feature.js",
    "validate:features": "node scripts/validate-features.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unified-proxy": "jest tests/unified-proxy.test.js",
    "test:deployment": "jest tests/deployment.test.js --testTimeout=30000",
    "test:post-deployment": "jest tests/post-deployment.test.js --testTimeout=60000",
    "test:all": "npm test && npm run test:deployment",
    "postdeploy": "node scripts/test-deployment.js"
  },
  "keywords": [
    "microservice",
    "coordinator",
    "registry",
    "service-discovery"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "@supabase/supabase-js": "^2.45.0",
    "openai": "^4.0.0",
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nock": "^13.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

